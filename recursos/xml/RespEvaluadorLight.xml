<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:eval="http://www.davivienda.com/xml/EvaluadorLight">
   <soapenv:Header/>
   <soapenv:Body>
      <eval:EvaluadorLightResponse>
         <Response>
            <DataHeader>
               <nombreOperacion>EvaluadorLight</nombreOperacion>
               <total>?</total>
               <caracterAceptacion>?</caracterAceptacion>
               <ultimoMensaje>?</ultimoMensaje>
               <idTransaccion>?</idTransaccion>
               <codMsgRespuesta>?</codMsgRespuesta>
               <!--Optional:-->
               <msgRespuesta>?</msgRespuesta>
            </DataHeader>
            <Data>
               <!--Optional:-->
               <solicitudResp>
                  <!--Optional:-->
                  <numeroSolicitud>?</numeroSolicitud>
                  <!--Optional:-->
                  <fechaEvaluacion>?</fechaEvaluacion>
                  <!--Optional:-->
                  <flagCampanaFirme>?</flagCampanaFirme>
                  <!--Optional:-->
                  <decisionComportamiento>?</decisionComportamiento>
                  <!--Optional:-->
                  <decisionEndeudamiento>?</decisionEndeudamiento>
                  <!--Optional:-->
                  <decisionOtorgamiento>?</decisionOtorgamiento>
                  <!--Optional:-->
                  <nivelDeAtribucion>?</nivelDeAtribucion>
                  <!--Optional:-->
                  <nivelDeAtribucionTasa>?</nivelDeAtribucionTasa>
                  <!--Optional:-->
                  <flagResolucionCasos>?</flagResolucionCasos>
                  <!--Optional:-->
                  <decisionSolicitudResp>
                     <!--Optional:-->
                     <DecisionCategory>?</DecisionCategory>
                     <!--Optional:-->
                     <DecisionText>?</DecisionText>
                     <!--Optional:-->
                     <sortedReasonCodeTableResp>
                        <!--Optional:-->
                        <I1>?</I1>
                        <!--Optional:-->
                        <I2>?</I2>
                        <!--Optional:-->
                        <I3>?</I3>
                        <!--Optional:-->
                        <I4>?</I4>
                        <!--Optional:-->
                        <I5>?</I5>
                     </sortedReasonCodeTableResp>
                     <!--Optional:-->
                     <scoreSolicitudResp>
                        <!--Optional:-->
                        <Score>?</Score>
                     </scoreSolicitudResp>
                  </decisionSolicitudResp>
                  <!--Optional:-->
                  <flagsAdicionalesResp>
                     <!--0 to 10 repetitions:-->
                     <flagAdicionalResp>
                        <!--Optional:-->
                        <NumeroDeFlagAdicional>?</NumeroDeFlagAdicional>
                        <!--Optional:-->
                        <Detalle>?</Detalle>
                        <!--Optional:-->
                        <Nombre>?</Nombre>
                     </flagAdicionalResp>
                  </flagsAdicionalesResp>
                  <!--Optional:-->
                  <flagsRequiereVerificacionResp>
                     <!--0 to 6 repetitions:-->
                     <flagRequiereVerificacionResp>
                        <!--Optional:-->
                        <secuenciaFlagRequiereVerificacion>?</secuenciaFlagRequiereVerificacion>
                        <!--Optional:-->
                        <flagRequiereVerificacion>?</flagRequiereVerificacion>
                     </flagRequiereVerificacionResp>
                  </flagsRequiereVerificacionResp>
                  <!--Optional:-->
                  <productosResp>
                     <!--0 to 10 repetitions:-->
                     <ProductoResp>
                        <!--Optional:-->
                        <decisionComportamiento>?</decisionComportamiento>
                        <!--Optional:-->
                        <decisionEndeudamiento>?</decisionEndeudamiento>
                        <!--Optional:-->
                        <decisionOtorgamiento>?</decisionOtorgamiento>
                        <!--Optional:-->
                        <idProducto>?</idProducto>
                        <!--Optional:-->
                        <montoMaximo>?</montoMaximo>
                        <!--Optional:-->
                        <montoMaximoContraoferta>?</montoMaximoContraoferta>
                        <!--Optional:-->
                        <montoMaximoSinCobertura>?</montoMaximoSinCobertura>
                        <!--Optional:-->
                        <montoMaximoSombra>?</montoMaximoSombra>
                        <!--Optional:-->
                        <aprobacionResp>
                           <!--Optional:-->
                           <banda>?</banda>
                           <!--Optional:-->
                           <color>?</color>
                           <!--Optional:-->
                           <limiteSobrecupo>?</limiteSobrecupo>
                           <!--Optional:-->
                           <montoAprobado>?</montoAprobado>
                           <!--Optional:-->
                           <montoAprobadoSinCobertura>?</montoAprobadoSinCobertura>
                           <!--Optional:-->
                           <plazoAprobado>?</plazoAprobado>
                           <!--Optional:-->
                           <plazoAprobadoSinCobertura>?</plazoAprobadoSinCobertura>
                           <!--Optional:-->
                           <sistemaAprobadoSinCobertura>?</sistemaAprobadoSinCobertura>
                           <!--Optional:-->
                           <valorGarantiaMinimo>?</valorGarantiaMinimo>
                           <!--Optional:-->
                           <calificacionOriginacion>?</calificacionOriginacion>
                        </aprobacionResp>
                        <!--Optional:-->
                        <scoreSolicitantesResp>
                           <!--0 to 6 repetitions:-->
                           <scoreSolicitanteResp>
                              <!--Optional:-->
                              <secuenciaScoreSolicitante>?</secuenciaScoreSolicitante>
                              <!--Optional:-->
                              <scoreSolicitante>?</scoreSolicitante>
                           </scoreSolicitanteResp>
                        </scoreSolicitantesResp>
                        <!--Optional:-->
                        <politicasProdResp>
                           <!--Optional:-->
                           <DecisionCategory>?</DecisionCategory>
                           <!--Optional:-->
                           <DecisionText>?</DecisionText>
                           <!--Optional:-->
                           <sortedReasonCodeTableResp>
                              <!--Optional:-->
                              <I1>?</I1>
                              <!--Optional:-->
                              <I2>?</I2>
                              <!--Optional:-->
                              <I3>?</I3>
                              <!--Optional:-->
                              <I4>?</I4>
                              <!--Optional:-->
                              <I5>?</I5>
                           </sortedReasonCodeTableResp>
                        </politicasProdResp>
                        <!--Optional:-->
                        <scoreProdResp>
                           <!--Optional:-->
                           <Score>?</Score>
                        </scoreProdResp>
                     </ProductoResp>
                  </productosResp>
               </solicitudResp>
            </Data>
         </Response>
      </eval:EvaluadorLightResponse>
   </soapenv:Body>
</soapenv:Envelope>