<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:prog="http://www.davivienda.com/xml/ProgramacionDebitoAutomatico">
   <soapenv:Header/>
   <soapenv:Body>
      <prog:programarDebitoAutomaticoResponse>
         <Response>
            <DataHeader>
               <nombreOperacion>?</nombreOperacion>
               <total>?</total>
               <caracterAceptacion>?</caracterAceptacion>
               <ultimoMensaje>?</ultimoMensaje>
               <idTransaccion>?</idTransaccion>
               <codMsgRespuesta>?</codMsgRespuesta>
               <!--Optional:-->
               <msgRespuesta>?</msgRespuesta>
            </DataHeader>
            <!--Optional:-->
            <Data>
               <valCodigoOperacion>?</valCodigoOperacion>
               <valTipoIdentificacion>?</valTipoIdentificacion>
               <numIdentificacion>?</numIdentificacion>
               <!--Optional:-->
               <registros>
                  <!--Zero or more repetitions:-->
                  <registro>
                     <numProducto>?</numProducto>
                     <valTipoProducto>?</valTipoProducto>
                     <valCodSubProducto>?</valCodSubProducto>
                     <valMensajeError>?</valMensajeError>
                     <valCodigoStatus>?</valCodigoStatus>
                     <valAlias>?</valAlias>
                     <valTipoPago>?</valTipoPago>
                     <valIndicadorProgramacion>?</valIndicadorProgramacion>
                     <valIndicadorSMS>?</valIndicadorSMS>
                     <!--Optional:-->
                     <valCampoFuturoTexto1>?</valCampoFuturoTexto1>
                     <!--Optional:-->
                     <valCampoFuturoTexto2>?</valCampoFuturoTexto2>
                     <!--Optional:-->
                     <numCampoFuturoValor1>?</numCampoFuturoValor1>
                     <!--Optional:-->
                     <numCampoFuturoValor2>?</numCampoFuturoValor2>
                  </registro>
               </registros>
            </Data>
         </Response>
      </prog:programarDebitoAutomaticoResponse>
   </soapenv:Body>
</soapenv:Envelope>